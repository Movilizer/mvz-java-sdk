<?xml version="1.0"?>
#* @vtlvariable name="context" type="com.movilizer.util.examples.charts.IChartData" *#
#* @vtlvariable name="utils" type="com.movilizer.util.MovilizerTemplateUtils" *#
#* @vtlvariable name="local" type="java.lang.String" *#
#* @vtlvariable name="global" type="java.lang.String" *#
#* @vtlvariable name="answer" type="java.lang.String" *#
#* @vtlvariable name="base64" type="java.lang.String" *#
#* @vtlvariable name="customizing" type="java.lang.String" *#
<MovilizerMovelet xmlns="http://movilitas.com/movilizer/v10"
                  moveletKey="JavaConnectorDemo"
                  moveletKeyExtension="$context.MoveletKeyExtension"
                  namespace="$context.Namespace"
                  name="HTML5 Demo"
                  moveletType="MULTI"
                  visible="true"
                  initialQuestionKey="Q0"
                  icon="81">

    <MovilizerQuestion type="0" title="Welcome to Java Connector HTML5 Demo" key="Q0">
        <MovilizerAnswer position="1" nextQuestionKey="Q1" key="Q0A1">
            <text>This movelet demonstrates
- charts
- tables with multiple cell colors
- order taking (HTML5):
    - dynamic calculations
    - hidable areas (accordeon)
    - typeahead (autocomplete)

            </text>
        </MovilizerAnswer>
        <onEnterAssignment><![CDATA[$context.saveFilesOnDevice("chart.js",
                                                               "line-chart.html",
                                                               "table.html")]]>
        </onEnterAssignment>
    </MovilizerQuestion>

    <!-- HTML View  for Line Chart -->
    <MovilizerQuestion type="12" title="Line Chart" key="Q1" backNavigationAllowed="false">
        <MovilizerAnswer nextQuestionKey="Q3" key="A_WEB_VIEW_LINE" dummyAnswer="false">
            <text>%URL%</text>
        </MovilizerAnswer>
        <onEnterAssignment>
            setPlaceholder("%URL%", $context.getLocalFilePath("line-chart.html"));
        </onEnterAssignment>
    </MovilizerQuestion>


    <!-- HTML View  for Table-->
    <MovilizerQuestion type="12" title="Table with multiple colors" key="Q3" backNavigationAllowed="false">
        <MovilizerAnswer nextQuestionKey="END" key="A_WEB_VIEW_TABLE" dummyAnswer="false">
            <text>%URL%</text>
        </MovilizerAnswer>
        <onEnterAssignment>
            setPlaceholder("%URL%", $context.getLocalFilePath("table.html"));
        </onEnterAssignment>
    </MovilizerQuestion>

    <syncDownloadAssignment>
        jIcon=$base64:&lt;>;
        setCustomizingProperty( $customizing:"icon.movelet81", jIcon);
    </syncDownloadAssignment>
</MovilizerMovelet>
